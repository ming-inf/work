alias open='cygstart'
alias reloadrc='. ~/.zshrc'
alias l='ls -l --color=auto'
alias ll='ls -la --color=auto'

alias ga='git add'
alias gcm='git commit -m'
alias gd='git diff'
alias gdc='git diff --cached'
alias gg='git log --graph --abbrev-commit --decorate --format=format:"%C(bold blue)%h%C(reset) %C(white)%s%C(reset) %C(bold green)(%ar)%C(reset) %an%C(reset)%C(bold yellow)%d%C(reset)" --all'
alias gs='git status'
